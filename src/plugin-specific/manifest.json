{
  "name": "Tag you're it",
  "description": "A browser tool for tagging words with exact definitions.",
  "version": "1.0.2",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://imdb.uib.no/"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "*.html",
    "*.css",
    "*.js"
  ],
  "browser_action": {
    "default_icon": "icon48.png"
  }
}
